	firefox	

super + f
	st -e lfrun
	
super + e
	st -e nvim
	
super + H
	st -e htop

super + t
	telegram-desktop

super + equal
	amixer set Master 4dB+

super + minus
	amixer set Master 4dB-

#super {equal, minus, BackSpace}
$	~/volume.sh {up, down, toggle}

